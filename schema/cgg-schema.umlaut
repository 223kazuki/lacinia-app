enum RelationshipStatus {
  SINGLE
  MARRIED
}

@lang/dot color red
@doc "This is my new type"
interface Person {
  id: ID
  name: String
  email: String?
  status: RelationshipStatus
}

interface Human {
  temperature: Float
}

type Employee : Person Human {
  salary: Float
  changeSalary(newSalary: Float): Float
}

@lang/lacinia identifier query
type QueryRoot {
  friends: Person[0..n]
}